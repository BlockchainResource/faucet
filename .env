#
# Backend input variables
#

# the name of the testnet
FAUCET_CHAIN=gaia-7000

# Google reCaptcha secret key
FAUCET_RECAPTCHA_SECRET_KEY=PASTESECRETKEY

# faucet backend url
# hostname and port should match VUE_APP_CLAIM_URL for frontend
FAUCET_PUBLIC_URL=127.0.0.1:8000

# how much faucetToken to send
FAUCET_AMOUNT_FAUCET=10faucetToken

# how much steak to send
FAUCET_AMOUNT_STEAK=1steak

# gaiacli account that holds the faucet funds
FAUCET_KEY=default

# gaiacli account password
FAUCET_PASS=1234567890

# full node that runs `gaiacli send`
FAUCET_NODE=http://localhost:46657

#
# Frontend input variables
#

# the name of the testnet
VUE_APP_CHAIN=gaia-7000

# Google reCaptcha site key
VUE_APP_RECAPTCHA_SITE_KEY=6LdqyV0UAAAAAEqgBxvSsDpL2aeTEgkz_VTz1Vi1

# faucet backend url (/claim)
# hostname and port should match FAUCET_PUBLIC_URL for backend
VUE_APP_CLAIM_URL=http://localhost:8000/claim

